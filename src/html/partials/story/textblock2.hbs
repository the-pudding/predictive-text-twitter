<div class='textblock'>
	<div class='textblock__prose'>
		{{#each doc.textblock2-before}}
		<p>{{{value}}}</p>
		{{/each}}
	</div>
	<div class='textblock__bullets'>
		<ul>
			{{#each doc.textblock2-bullets}}
			<li>{{{value}}}</li>
			{{/each}}
		</ul>
	</div>
	<div class='textblock__prose'>
		{{#each doc.textblock2-after}}
		<p>{{{value}}}</p>
		{{/each}}
	</div>
</div>
